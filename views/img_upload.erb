<form class="inline" action="/img" method="post" enctype="multipart/form-data">
  <div>
    <label for="img_file">Choose an image:</label>
    <input name="img_file" type="file" accept="image/*" value="Choose">
  </div>
  <input type="submit" value="Upload">
</form>

<form class="inline" action='/' method="get">
  <input type="submit" value="Cancel">
</form>
